    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.0",
    "expo-font": "~14.0.0",
    "expo-image": "~3.0.0",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.0",
    "expo-router": "~6.0.23",
    "expo-status-bar": "~3.0.0",
